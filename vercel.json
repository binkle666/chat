{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "pages/api/socket.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_USE_CUSTOM_SERVER": "false"
  },
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  },
  "installCommand": "npm ci --legacy-peer-deps"
}
